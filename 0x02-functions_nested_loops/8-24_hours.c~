#include "main.h"

/**
 * jack_bauer - print every minute of the day of jack bauer
 *
 */

void jack_bauer(void)
{
	int time, sec;

	for (time = 0; time < 24; time++)
	{
		for (sec = 0; sec < 60; sec++)
		{
			_putchar((time / 10) + '0');
			_putchar((time % 10) + '0');
			_putchar(':');
			_putchar((sec / 10) + '0');
			_putchar((sec % 10) + '0');
		}
	}
}
